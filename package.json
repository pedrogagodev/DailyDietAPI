{
  "name": "dailydietapi",
  "version": "1.0.0",
  "main": "build/server.js",
  "type": "module",
  "scripts": {
    "start:dev": "tsx watch src/server.ts",
    "start:prod": "node build/server.js",
    "build": "tsup src --out-dir build",
    "migrate:up": "tsx node_modules/.bin/node-pg-migrate up --migrations-dir database/migrations",
    "migrate:up:prod": "node-pg-migrate up --migrations-dir build/database/migrations --config-value db",
    "migrate:down": "tsx node_modules/.bin/node-pg-migrate down --migrations-dir database/migrations",
    "migrate:down:prod": "node-pg-migrate down --migrations-dir build/database/migrations --config-value db",
     "migrate:create": "tsx node_modules/.bin/node-pg-migrate create minha_migracao --migrations-dir database/migrations --migration-file-language ts",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui --coverage.enabled=true"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@fastify/cookie": "11.0.2",
    "@fastify/jwt": "9.0.4",
    "bcrypt": "5.1.1",
    "config": "3.3.12",
    "dotenv": "16.4.7",
    "fastify": "5.2.1",
    "pg": "8.13.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@types/bcrypt": "5.0.2",
    "@types/node": "22.13.9",
    "@types/pg": "8.11.11",
    "@vitest/coverage-v8": "3.0.8",
    "@vitest/ui": "3.0.8",
    "node-pg-migrate": "7.9.1",
    "tsup": "8.4.0",
    "tsx": "4.19.3",
    "typescript": "5.8.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.0.8",
    "zod": "3.24.2"
  }
}
